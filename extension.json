{
	"name": "PluggableAuth",
	"version": "6.0-dev",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Cindy.cicalese Cindy Cicalese]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:PluggableAuth",
	"descriptionmsg": "pluggableauth-desc",
	"type": "other",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\PluggableAuth\\": "includes/"
	},
	"SpecialPages": {
		"PluggableAuthLogin": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthLogin"
	},
	"MessagesDirs": {
		"PluggableAuth": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"PluggableAuthAlias": "includes/PluggableAuth.alias.php"
	},
	"callback": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::onRegistration",
	"Hooks": {
		"TitleReadWhitelist": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::onTitleReadWhitelist",
		"UserLogoutComplete": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::deauthenticate",
		"AuthChangeFormFields": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::onAuthChangeFormFields",
		"PersonalUrls": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::modifyLoginURLs",
		"BeforeInitialize": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::doBeforeInitialize",
		"LocalUserCreated": "MediaWiki\\Extension\\PluggableAuth\\PluggableAuthHooks::onLocalUserCreated"
	},
	"AuthManagerAutoConfig": {
		"primaryauth": {
			"PluggableAuthPrimaryAuthenticationProvider": {
				"class": "MediaWiki\\Extension\\PluggableAuth\\PrimaryAuthenticationProvider",
				"sort": 0
			}
		}
	},
	"PluggableAuthLoginSpecialPages": [
		"PluggableAuthLogin",
		"Userlogin"
	],
	"config": {
		"PluggableAuth_EnableAutoLogin": false,
		"PluggableAuth_EnableLocalLogin": false,
		"PluggableAuth_EnableLocalProperties": false,
		"PluggableAuth_ButtonLabel": null,
		"PluggableAuth_ExtraLoginFields": []
	},
	"manifest_version": 1
}
